<%= form_for [@club, @meeting] do |f| %>

  <%= f.label :title %><br>
	<%= f.text_field :title %><br>

  <%= f.label :date %><br>
	<%= f.date_field :date, class: 'field' %><br>

  <%= f.label :location %><br>
	<%= f.text_field :location %><br>

  <%= f.label :address %><br>
  <%= f.text_field :address %><br>

  <%= f.label :details %><br>
  <%= f.text_area :details, size: "60x12" %><br>

  <%= f.label :host %><br>
  <%= f.text_field :host %><br>

  <%= f.label :time %><br>
  <%= f.text_field :time %><br>

  <%= f.label :attending %><br>
  <%= f.text_field :attending %><br>

  <%= f.label :meeting_image %><br>
  <%= f.text_field :meeting_image,  size: "60x1" %><br>


	<%= f.submit 'Submit', class: 'enter' %>

<% end %>
<!-- ERICA: Nice use of the 'size' parameter to customize the form -->
