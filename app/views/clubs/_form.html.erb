<%= form_for @club do |f| %>

	<%= f.label :name %><br>
	<%= f.text_field :name %><br>
  <%= f.label :description %><br>
	<%= f.text_area :description, size: "60x8" %><br>
  <%= f.label :img_url %><br>
  <%= f.text_field :img_url, size: "60x1"  %><br>
	<%= f.submit 'submit', class: 'enter' %>
<% end %>
